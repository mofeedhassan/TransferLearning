   <SOURCE>
      <ID>dbpedia_FU</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type dbpedia-owl:Book.</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>dbpedia-prop:name</PROPERTY>
      <PROPERTY>dbpedia:wikiPageRedirects</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>gutenberg</ID>
      <ENDPOINT>http://www4.wiwiss.fu-berlin.de/gutendata/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type dctype:Text.</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>dc:alternative</PROPERTY>
   </TARGET>
   <METRIC>MAX(levenshtein(a.rdfs:label, b.rdfs:label), MAX(levenshtein(a.rdfs:label, b.dc:alternative), levenshtein(a.dbpedia-prop:name, b.rdfs:label), Levenshtein(a.dbpedia-prop:name, b.dc:alternative), Levenshtein(, b.rdfs:label), Levenshtein(, b.dc:alternative))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.97</THRESHOLD>
      <FILE>dbpedia_gut_texts.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.97</THRESHOLD>
      <FILE>dbpedia_gut_texts.nt</FILE>
      <RELATION/>
   </REVIEW>
