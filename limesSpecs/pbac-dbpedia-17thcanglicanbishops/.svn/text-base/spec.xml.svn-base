<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbpp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/ontology/bibo/</NAMESPACE>
      <LABEL>bibo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.kanzaki.com/ns/whois#</NAMESPACE>
      <LABEL>whois</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://open.vocab.org/terms/</NAMESPACE>
      <LABEL>ov</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
      <LABEL>dcterms</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/class/yago/</NAMESPACE>
      <LABEL>yago</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/vocab/bio/0.1/</NAMESPACE>
      <LABEL>bio</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>printed-book-auction-catalogues</ID>
      <ENDPOINT>http://api.talis.com/stores/pbac/services/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>{ ?a rdf:type foaf:Agent . } UNION { ?a a foaf:Person } .</RESTRICTION>
      <PROPERTY>owl:sameAs/rdfs:label</PROPERTY>
      <PROPERTY>whois:born</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>{ ?b rdf:type yago:17th-centuryAnglicanBishops . } UNION { ?b a yago:EjectedEnglishMinistersOf1662 . } UNION { ?b a yago:EnglishAnglicanPriests . } UNION { ?b a yago:16th-centuryPhilosophers } UNION { ?b a yago:17th-centuryPhilosophers }</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>dcterms:subject</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.5*MAX(trigrams(b.rdfs:label, a\owl:sameAs.rdfs:label)), 0.5*Equality(a.whois:born, b.dcterms:subject))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.90</THRESHOLD>
      <FILE>pbac_dbpedia_links_accepted.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.90</THRESHOLD>
      <FILE>pbac_dbpedia_links_accepted.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
