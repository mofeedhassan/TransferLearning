<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://umbel.org/umbel/sc/</NAMESPACE>
      <LABEL>umbel-sc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/triplify/</NAMESPACE>
      <LABEL>linkedgeodata</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/property/</NAMESPACE>
      <LABEL>lgdp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/ontology/</NAMESPACE>
      <LABEL>lgdo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>geo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>wgs84</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.geonames.org/ontology#</NAMESPACE>
      <LABEL>geonames</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2001/XMLSchema#</NAMESPACE>
      <LABEL>xsd</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://airports.dataincubator.org/schema/</NAMESPACE>
      <LABEL>airport</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www4.wiwiss.fu-berlin.de/factbook/ns#</NAMESPACE>
      <LABEL>factbook</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>factbook</ID>
      <ENDPOINT>http://www4.wiwiss.fu-berlin.de/factbook/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a a factbook:Country.</RESTRICTION>
      <PROPERTY>factbook:internetcountrycode</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>linkedgeodata</ID>
      <ENDPOINT>http://linkedgeodata.org/sparql/</ENDPOINT>
      <GRAPH/>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b a lgdo:Country. ?b lgdo:official_name ?n.</RESTRICTION>
      <PROPERTY>lgdp:country_code_iso3166_1_alpha_2</PROPERTY>
   </TARGET>
   <METRIC>MAX(Levenshtein(, b.lgdp:country_code_iso3166_1_alpha_2))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.9999999999</THRESHOLD>
      <FILE>factbook/links.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.9999999999</THRESHOLD>
      <FILE>factbook/links.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
