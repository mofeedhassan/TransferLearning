<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://umbel.org/umbel/sc/</NAMESPACE>
      <LABEL>umbel-sc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/triplify/</NAMESPACE>
      <LABEL>linkedgeodata</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/property/</NAMESPACE>
      <LABEL>lgdp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/ontology/</NAMESPACE>
      <LABEL>lgdo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>geo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>wgs84</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.geonames.org/ontology#</NAMESPACE>
      <LABEL>geonames</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2001/XMLSchema#</NAMESPACE>
      <LABEL>xsd</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://airports.dataincubator.org/schema/</NAMESPACE>
      <LABEL>airport</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>airportdata</ID>
      <ENDPOINT>http://api.talis.com/stores/airports/services/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>5</PAGESIZE>
      <RESTRICTION>{?a a airport:Balloonport} UNION {?a a airport:Heliport} UNION {?a a airport:SeaplaneBase} UNION {?a a airport:LargeAirport} UNION {?a a airport:MediumAirport} UNION {?a a airport:SmallAirport}.</RESTRICTION>
      <PROPERTY>dbp:iata</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>linkedgeodata</ID>
      <ENDPOINT>http://linkedgeodata.org/sparql/</ENDPOINT>
      <GRAPH/>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>{?b a lgdo:Aeroway} UNION {?b a lgdo:Aerodrome} UNION {?b a lgdo:Airport} UNION {?b a lgdo:Aerialway}.</RESTRICTION>
      <PROPERTY>lgdp:iata</PROPERTY>
   </TARGET>
   <METRIC>ADD(1*Levenshtein(a.dbp:iata, b.lgdp:iata))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.9999999999</THRESHOLD>
      <FILE>airportdata/links.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.9999999999</THRESHOLD>
      <FILE>airportdata/links.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
