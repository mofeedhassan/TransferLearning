<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbpedia-prop</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://rs.tdwg.org/dwc/terms/</NAMESPACE>
      <LABEL>dwc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
      <LABEL>dc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/resource/</NAMESPACE>
      <LABEL>dbpedia</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugbank/</NAMESPACE>
      <LABEL>drugbank</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://eunis.eea.europa.eu/rdf/species-schema.rdf#</NAMESPACE>
      <LABEL>eunis</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www4.wiwiss.fu-berlin.de/cordis/resource/cordis/</NAMESPACE>
      <LABEL>cordis</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
      <LABEL>skos</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>DBpedia</ID>
      <ENDPOINT>http://live.dbpedia.org/sparql/</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type dbpedia-owl:Species</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>dbpedia-owl:genus/rdfs:label</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>EUNIS</ID>
      <ENDPOINT>http://lsd.taxonconcept.org/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type eunis:SpeciesSynonym</RESTRICTION>
      <PROPERTY>eunis:binomialName</PROPERTY>
      <PROPERTY>dwc:genus</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.5*Levenshtein(a.rdfs:label, b.eunis:binomialName), 0.5*Levenshtein(, b.dwc:genus))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>dbpedia_eunis_species.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>dbpedia_eunis_species.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
