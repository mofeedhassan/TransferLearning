<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbpp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://data.ordnancesurvey.co.uk/ontology/spatialrelations/</NAMESPACE>
      <LABEL>spatial</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://climb.dataincubator.org/vocabs/climb/</NAMESPACE>
      <LABEL>climb</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://data.ordnancesurvey.co.uk/ontology/50kGazetteer/</NAMESPACE>
      <LABEL>gazetteer</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://transport.data.gov.uk/def/naptan/</NAMESPACE>
      <LABEL>naptan</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.geonames.org/ontology#</NAMESPACE>
      <LABEL>geonames</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://open.vocab.org/terms/</NAMESPACE>
      <LABEL>ov</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>geo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://statistics.data.gov.uk/def/administrative-geography/</NAMESPACE>
      <LABEL>dgadmingeo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/class/yago/</NAMESPACE>
      <LABEL>yago</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
      <LABEL>skos</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://data.ordnancesurvey.co.uk/ontology/admingeo/</NAMESPACE>
      <LABEL>admingeo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://transport.data.gov.uk/def/traffic/</NAMESPACE>
      <LABEL>traffic</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://metoffice.dataincubator.org/vocabs/metoffice/</NAMESPACE>
      <LABEL>metoffice</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://ontologi.es/rail/vocab#</NAMESPACE>
      <LABEL>rail</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>data-incubator-metoffice</ID>
      <ENDPOINT>http://api.talis.com/stores/metoffice/services/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type metoffice:MetOfficeLocation .</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>geo:lat</PROPERTY>
      <PROPERTY>geo:long</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>ordnance-survey-linked-data</ID>
      <ENDPOINT>http://kwijibo.talis.com/ordnance-survey-linked-data-places/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type gazetteer:NamedPlace ; geo:lat ?lat ; geo:long ?long . { ?b gazetteer:featureType gazetteer:Other } UNION { ?b gazetteer:featureType gazetteer:OtherSettlement }</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>geo:lat</PROPERTY>
      <PROPERTY>geo:long</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.5*Equality(a.rdfs:label, b.rdfs:label), 0.5*WGS84(a.geo:lat, b.geo:lat))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.90</THRESHOLD>
      <FILE>metoffice_ordnance-survey_other-settlements_locations.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.90</THRESHOLD>
      <FILE>metoffice_ordnance-survey_other-settlements_locations.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
