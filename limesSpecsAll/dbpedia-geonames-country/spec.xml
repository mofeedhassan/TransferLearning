      <NAMESPACE>http://data.linkedmdb.org/resource/movie/</NAMESPACE>
      <LABEL>linkedmdb</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/property/</NAMESPACE>
      <LABEL>lgdp</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type dbpedia-owl:Country.</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>dbpedia:wikiPageRedirects</PROPERTY>
      <PROPERTY>wgs84:lat</PROPERTY>
      <PROPERTY>wgs84:long</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>geonames</ID>
      <ENDPOINT>http://160.45.137.69:8890/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>{?b geonames:featureCode geonames:PCLI} UNION {?b geonames:featureCode geonames:PCLD}.</RESTRICTION>
      <PROPERTY>geonames:name</PROPERTY>
      <PROPERTY>geonames:alternateName</PROPERTY>
      <PROPERTY>wgs84:lat</PROPERTY>
      <PROPERTY>wgs84:long</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.5*MAX(Levenshtein(a.rdfs:label, b.geonames:name), Levenshtein(a.rdfs:label, b.geonames:alternateName), Levenshtein(, b.geonames:name)), 0.5*MAX(WGS84(a.wgs84:lat, b.wgs84:lat)))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.92</THRESHOLD>
      <FILE>dbpedia-geonames-country.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.92</THRESHOLD>
      <FILE>dbpedia-geonames-country.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>TAB</OUTPUT>
</LIMES>
