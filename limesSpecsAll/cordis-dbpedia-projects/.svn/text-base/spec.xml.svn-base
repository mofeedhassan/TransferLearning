<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbpedia-prop</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
      <LABEL>dc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www4.wiwiss.fu-berlin.de/cordis/resource/cordis/</NAMESPACE>
      <LABEL>cordis</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a dbpedia-prop:wikiPageUsesTemplate &lt;http://dbpedia.org/resource/Template:Research-Project&gt;</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>dbpedia-prop:redirect</PROPERTY>
      <PROPERTY>dbpedia-prop:title</PROPERTY>
      <PROPERTY>dbpedia-prop:name</PROPERTY>
      <PROPERTY>dbpedia-prop:website</PROPERTY>
      <PROPERTY>dbpedia-prop:projectreference</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>cordis</ID>
      <ENDPOINT>http://www4.wiwiss.fu-berlin.de/cordis/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type cordis:Project</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>cordis:acronym</PROPERTY>
      <PROPERTY>foaf:page</PROPERTY>
      <PROPERTY>cordis:reference</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.5*MAX(Levenshtein(a.rdfs:label, b.rdfs:label), Levenshtein(, b.rdfs:label), Levenshtein(a.dbpedia-prop:title, b.rdfs:label), Levenshtein(a.rdfs:label, b.cordis:acronym), Levenshtein(, b.cordis:acronym), Levenshtein(a.dbpedia-prop:title, b.cordis:acronym), Levenshtein(a.dbpedia-prop:name, b.cordis:acronym)), 0.5*MAX(Equality(a.dbpedia-prop:website, b.foaf:page), Equality(a.dbpedia-prop:projectreference, b.cordis:reference), Equality(a.dbpedia-prop:projectreference, b.cordis:reference), Equality(a.dbpedia-prop:projectreference, b.cordis:reference), Equality(a.dbpedia-prop:projectreference, b.cordis:reference), Equality(a.dbpedia-prop:projectreference, b.cordis:reference), Equality(a.dbpedia-prop:projectreference, b.cordis:reference), Equality(a.dbpedia-prop:projectreference, b.cordis:reference), Equality(a.dbpedia-prop:projectreference, b.cordis:reference)))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>cordis_dbpedia_projects.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>cordis_dbpedia_projects.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>TAB</OUTPUT>
</LIMES>
