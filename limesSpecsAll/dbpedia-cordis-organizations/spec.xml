<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbpedia-prop</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://rs.tdwg.org/dwc/terms/</NAMESPACE>
      <LABEL>dwc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
      <LABEL>dc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/resource/</NAMESPACE>
      <LABEL>dbpedia</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugbank/</NAMESPACE>
      <LABEL>drugbank</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://eunis.eea.europa.eu/rdf/species-schema.rdf#</NAMESPACE>
      <LABEL>eunis</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www4.wiwiss.fu-berlin.de/cordis/resource/cordis/</NAMESPACE>
      <LABEL>cordis</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
      <LABEL>skos</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://rdf.geospecies.org/ont/geospecies#</NAMESPACE>
      <LABEL>geospecies</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>DBpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type dbpedia-owl:Organisation</RESTRICTION>
      <PROPERTY>dbpedia-prop:wikiPageRedirects/rdfs:label</PROPERTY>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>foaf:homepage</PROPERTY>
      <PROPERTY>dbpedia-prop:coordinator/rdfs:label</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>CORDIS</ID>
      <ENDPOINT>http://www4.wiwiss.fu-berlin.de/cordis/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type cordis:Organization</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>cordis:url</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.5*MAX(Jaro(b.rdfs:label, a\dbpedia-prop:wikiPageRedirects.rdfs:label), trigrams(a.rdfs:label, b.rdfs:label)), 0.5*MAX(trigrams(, ), trigrams(b.rdfs:label, a\dbpedia-prop:coordinator.rdfs:label)))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>cordis_dbpedia_organizations.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>cordis_dbpedia_organizations.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>TAB</OUTPUT>
</LIMES>
