<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://umbel.org/umbel/sc/</NAMESPACE>
      <LABEL>umbel-sc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/triplify/</NAMESPACE>
      <LABEL>linkedgeodata</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/property/</NAMESPACE>
      <LABEL>lgdp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/ontology/</NAMESPACE>
      <LABEL>lgdo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>geo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>wgs84</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.geonames.org/ontology#</NAMESPACE>
      <LABEL>geonames</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2001/XMLSchema#</NAMESPACE>
      <LABEL>xsd</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.fao.org/countryprofiles/geoinfo/geopolitical/resource/</NAMESPACE>
      <LABEL>fao</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>fao</ID>
      <ENDPOINT>http://lgd.aksw.org:5678/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a a fao:self_governing.</RESTRICTION>
      <PROPERTY>fao:nameList</PROPERTY>
      <PROPERTY>fao:nameShort</PROPERTY>
      <PROPERTY>fao:nameOfficial</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>linkedgeodata</ID>
      <ENDPOINT>http://linkedgeodata.org/sparql/</ENDPOINT>
      <GRAPH/>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b a lgdo:Country. ?b wgs84:lat ?lat. ?b wgs84:long ?long.</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
   </TARGET>
   <METRIC>MAX(MAX(Trigram(a.fao:nameList, b.rdfs:label)), MAX(Trigram(a.fao:nameShort, b.rdfs:label)), MAX(Trigram(a.fao:nameOfficial, b.rdfs:label)))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.95</THRESHOLD>
      <FILE>fao/links.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.95</THRESHOLD>
      <FILE>fao/links.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
