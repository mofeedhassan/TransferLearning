   <SOURCE>
      <ID>greater-manchester-bus-timetable-linked-data</ID>
      <ENDPOINT>http://linkedmanchester.org/sparql.xml?_per_page=1000</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type &lt;http://vocab.org/transit/terms/Operator&gt; .</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>{ ?b dcterms:subject &lt;http://dbpedia.org/resource/Category:Bus_operators_in_England&gt; . } UNION { ?b dcterms:subject &lt;http://dbpedia.org/resource/Category:Transport_in_Greater_Manchester&gt; }</RESTRICTION>
      <PROPERTY>rdfs:label[@lang = 'en']</PROPERTY>
   </TARGET>
   <METRIC>ADD(1*trigrams(a.rdfs:label, b.rdfs:label[@lang = 'en']))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.90</THRESHOLD>
      <FILE>linkedmanchester-dbpedia-busoperators.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.90</THRESHOLD>
