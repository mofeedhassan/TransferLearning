<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>datasemanticweborg</ID>
      <ENDPOINT>http://data.semanticweb.org/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type foaf:Person</RESTRICTION>
      <PROPERTY>foaf:name</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>DBLP</ID>
      <ENDPOINT>http://dblp.l3s.de/d2r/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type foaf:Agent</RESTRICTION>
      <PROPERTY>foaf:name</PROPERTY>
   </TARGET>
   <METRIC>MAX(Jaro(a.foaf:name, b.foaf:name))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.90</THRESHOLD>
      <FILE>researcher_links_accepted.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.90</THRESHOLD>
      <FILE>researcher_links_accepted.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
