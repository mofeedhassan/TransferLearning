<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/elements/1.1/</NAMESPACE>
      <LABEL>dc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://data.semanticweb.org/ns/swc/ontology#</NAMESPACE>
      <LABEL>swc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>source_a</ID>
      <ENDPOINT>http://eculture2.cs.vu.nl:8890/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type foaf:Person</RESTRICTION>
      <PROPERTY>foaf:name</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>source_b</ID>
      <ENDPOINT>http://dblp.l3s.de/d2r/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type foaf:Agent</RESTRICTION>
      <PROPERTY>foaf:name</PROPERTY>
   </TARGET>
   <METRIC>ADD(1*Levenshtein(a.foaf:name, b.foaf:name))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.95</THRESHOLD>
      <FILE>links.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.95</THRESHOLD>
      <FILE>links.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>TAB</OUTPUT>
</LIMES>
